        "symfony/filesystem": "~2.2,>=2.2.1",
        "symfony/finder": "~2.2,>=2.2.1",
        "symfony/form": "~2.2,>=2.2.1",
        "symfony/http-foundation": "~2.2,>=2.2.1",
        "symfony/http-kernel": "~2.2,>=2.2.1",
        "symfony/locale": "~2.2,>=2.2.1",
        "symfony/monolog-bridge": "~2.2,>=2.2.1",
